<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Project2</display-name>
  <servlet>
  	<servlet-name>Portfolio</servlet-name>
  	<servlet-class>csci310.servlets.Portfolio</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Portfolio</servlet-name>
	<url-pattern>/portfolio</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>User</servlet-name>
  	<servlet-class>csci310.servlets.User</servlet-class></servlet>
  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>/user</url-pattern></servlet-mapping>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>csci310.servlets.Login</servlet-class></servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<servlet-class>csci310.servlets.Logout</servlet-class></servlet>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>